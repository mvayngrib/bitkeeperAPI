{
  "name": "bitkeeper-client-js",
  "version": "0.0.1",
  "description": "Tradle bitkeeper API",
  "main": "index.js",
  "bin": {
    "keep": "./app.js"
  },
  "scripts": {
    "jsbeautify": "js-beautify -r *.js --config=node_modules/tradle-style/.jsbeautifyrc",
    "jscs": "jscs *.js --config=node_modules/tradle-style/.jscsrc",
    "jshint": "jshint *.js --config=node_modules/tradle-style/.jshintrc"
  },
  "pre-commit": [
    "jscs",
    "jshint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tradle/bitkeeper-client-js.git"
  },
  "bugs": {
    "url": "https://github.com/tradle/bitkeeper-client-js/issues"
  },
  "author": "Mark Vayngrib <mark.vayngrib@lablz.com> (http://github.com/mvayngrib)",
  "license": "MIT",
  "dependencies": {
    "chained-obj": "git://github.com/tradle/chained-obj",
    "minimist": "^1.1.1",
    "q": "^1.1.2",
    "request": "^2.51.0",
    "simple-get": "^1.3.0",
    "tradle-utils": "^0.1.0"
  },
  "devDependencies": {
    "jscs": "^1.10.0",
    "jshint": "^2.6.0",
    "pre-commit": "0.0.11",
    "tradle-style": "latest",
    "which": "^1.0.8"
  }
}
